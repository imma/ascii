#!/usr/bin/env bash

function version {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"

  local ver_asciinema="$(asciinema --version 2>/dev/null | awk '{print $2}' || true)"
  jq -n --arg asciinema "$ver_asciinema" '{asciinema: $asciinema}'
}

version "$@"
